{
  "name": "frontend",
  "scripts": {
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/devServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/prodServer.js",
    "clean:dist": "rimraf ./dist",
    "lint": "eslint ./src ./tools ./webpackConfig \"./*.js\"",
    "test": "cross-env NODE_ENV=testing jest --watch",
    "test:unwatch": "cross-env NODE_ENV=testing jest",
    "test:debug": "cross-env NODE_ENV=testing node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "update:snapshot": "cross-env NODE_ENV=testing jest --updateSnapshot",
    "predev": "babel-node ./tools/startMessage.js",
    "dev": "run-p serve:src test:watch",
    "prebuild": "run-s clean:dist test:unwatch",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "start": "npm run serve:src",
    "browsers": "babel-node ./tools/printBrowsersList"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
